{
	// for biome
	"editor.defaultFormatter": "biomejs.biome",
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit"
	},
	"prettier.enable": false,
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	}
}
